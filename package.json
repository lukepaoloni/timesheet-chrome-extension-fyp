{
    "name": "timesheet-api",
    "version": "0.0.1",
    "description": "The API for the timesheet chrome extension.",
    "author": "Luke Paoloni",
    "license": "MIT",
    "scripts": {
        "build": "tsc -p tsconfig.build.json",
        "format": "prettier --write \"src/**/*.ts\"",
        "start": "node build/src/main.js",
        "start:dev": "nodemon",
        "start:debug": "nodemon --config nodemon-debug.json",
        "prestart:prod": "rimraf build && tsc --noEmitOnError --moduleResolution node",
        "start:prod": "node build/src/main.js",
        "lint": "tslint -p tsconfig.json -c tslint.json",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "jest --config ./test/jest-e2e.json",
        "start:hmr": "node build/server",
        "webpack": "webpack --config webpack.config.js",
        "seed": "nps db.seed.all"
    },
    "dependencies": {
        "@nestjs/common": "^5.4.0",
        "@nestjs/core": "^5.4.0",
        "@nestjs/graphql": "^5.5.1",
        "@nestjs/jwt": "^0.2.1",
        "@nestjs/passport": "^5.1.0",
        "@nestjs/swagger": "^2.5.1",
        "@nestjs/typeorm": "^5.2.2",
        "@nestjs/websockets": "^5.7.3",
        "@types/express-session": "^1.15.12",
        "@types/passport-github": "^1.1.4",
        "@types/passport-google-oauth": "^1.0.39",
        "@types/web-push": "^3.3.0",
        "apollo-server-express": "^2.3.1",
        "axios": "^0.18.0",
        "bcrypt": "^3.0.5",
        "class-transformer": "^0.2.0",
        "compression": "^1.7.3",
        "csurf": "^1.9.0",
        "dotenv": "^6.2.0",
        "express-rate-limit": "^3.3.2",
        "express-session": "^1.15.6",
        "faker": "^4.1.0",
        "firebase": "^5.8.0",
        "glob": "^7.1.3",
        "graphql": "^14.1.1",
        "graphql-tools": "^4.0.3",
        "helmet": "^3.15.0",
        "moment": "^2.23.0",
        "mongodb": "^3.1.12",
        "nest-type-graphql": "^0.1.2",
        "nps": "^5.9.3",
        "passport": "^0.4.0",
        "passport-bitbucket-oauth2": "^0.1.2",
        "passport-github": "^1.1.0",
        "passport-google-oauth": "^1.0.0",
        "passport-google-oauth20": "^1.0.0",
        "passport-http-bearer": "^1.0.1",
        "passport-jwt": "^4.0.0",
        "reflect-metadata": "^0.1.12",
        "rimraf": "^2.6.2",
        "rxjs": "^6.2.2",
        "socket.io": "^2.2.0",
        "stringcaster": "^0.3.1",
        "ts-transformer-keys": "^0.3.4",
        "typeorm": "^0.2.11",
        "typeorm-seeding": "^1.0.0-beta.6",
        "typescript": "^3.0.1",
        "typescript-stringcaster": "^1.0.2",
        "web-push": "^3.3.3"
    },
    "devDependencies": {
        "@nestjs/testing": "^5.1.0",
        "@types/axios": "^0.14.0",
        "@types/bcrypt": "^3.0.0",
        "@types/express": "^4.16.0",
        "@types/faker": "^4.1.5",
        "@types/jest": "^23.3.1",
        "@types/node": "^10.7.1",
        "@types/supertest": "^2.0.5",
        "jest": "^23.5.0",
        "nodemon": "^1.18.3",
        "prettier": "^1.14.2",
        "supertest": "^3.1.0",
        "ts-jest": "^23.1.3",
        "ts-loader": "^4.4.2",
        "ts-node": "^7.0.1",
        "tsconfig-paths": "^3.5.0",
        "tsconfig-paths-webpack-plugin": "^3.2.0",
        "tslint": "5.11.0",
        "webpack": "^4.29.0",
        "webpack-cli": "^3.2.1",
        "webpack-node-externals": "^1.7.2"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": "src",
        "testRegex": ".spec.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "coverageDirectory": "../coverage",
        "testEnvironment": "node",
        "moduleNameMapper": {
            "@app/(.*)$": "<rootDir>/src/app/$1",
            "@user/(.*)$": "<rootDir>/src/user/$1",
            "@db/(.*)$": "<rootDir>/src/db/$1",
            "@shared/(.*)$": "<rootDir>/src/shared/$1",
            "@client/(.*)$": "<rootDir>/src/client/$1",
            "@project/(.*)$": "<rootDir>/src/project/$1"
        }
    }
}
